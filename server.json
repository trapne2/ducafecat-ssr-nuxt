{
    "name": "ducafecat-vue-ssr-nuxt",
    "script": "./node_modules/nuxt/bin/nuxt-start",
    "cwd": "./",
    "watch": [
        ".nuxt"
    ],
    "ignore_watch": [
        "node_modules",
        "logs"
    ],
    "watch_options": {
        "followSymlinks": false
    },
    "error_file": "./logs/app-err.log",
    "out_file": "./logs/app-out.log",
    "env": {
        "NODE_ENV": "production"
    }
}